# AI provider keys (client-side for local development only)
# NOTE: Vite exposes VITE_* variables to browser bundles.
# Do not use production secrets in direct client builds.
VITE_GEMINI_API_KEY=
VITE_CLAUDE_API_KEY=

# Optional provider model overrides
VITE_GEMINI_MODEL=gemini-2.5-flash
VITE_CLAUDE_MODEL=claude-3-5-haiku-latest

# Provider orchestration settings
VITE_AI_PROVIDER_TIMEOUT_MS=6000
VITE_AI_COOLDOWN_MS=30000
